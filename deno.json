{
  "lock": false,
  "tasks": {
    "check": "deno fmt --check && deno lint && deno check **/*.ts && deno check **/*.tsx",
    "start": "deno run -A  main.ts",
    "dev": "export ENV=development && deno run -A --watch=static/,routes/,components/,islands/ dev.ts --host",
    "build": "deno run -A dev.ts build",
    "docs": "deno run --allow-read --allow-write swagger.js",
    "fakebuild": "echo 'fake build'",
    "preview": "deno run -A main.ts",
    "update": "deno run -A -r https://fresh.deno.dev/update .",
    "decode:msig": "deno run -A scripts/decodeSRC20Msig.ts",
    "decode:olga": "deno run -A scripts/decodeSRC20Olga.ts",
    "get:btcinfo": "deno run -A scripts/getBitcoinNodeInfo.ts"
  },
  "lint": { "rules": { "tags": ["fresh", "recommended"] } },
  "exclude": ["**/_fresh/*", "**/server/**/*"],
  "nodeModulesDir": "auto",
  "imports": {
    "$/": "./",
    "$Big": "https://esm.sh/big.js@6.2.1",
    "$components/": "./components/",
    "$dayjs/": "https://esm.sh/dayjs@1.11.10/",
    "$fresh/": "https://deno.land/x/fresh@1.6.8/",
    "$fresh_charts/": "https://deno.land/x/fresh_charts@0.3.1/",
    "$handlers/": "./routes/handlers/",
    "$islands/": "./islands/",
    "$lib/": "./lib/",
    "$mysql/": "https://deno.land/x/mysql@v2.12.1/",
    "$routes/": "./routes/",
    "$server/": "./server/",
    "$std/": "https://deno.land/std@0.212.0/",
    "$workers/": "./workers/",
    "@iconify/react": "npm:@iconify/react@4.1.1",
    "@preact/signals": "https://esm.sh/*@preact/signals@1.2.2",
    "@preact/signals-core": "https://esm.sh/*@preact/signals-core@1.5.1",
    "@udibo/http-error": "jsr:@udibo/http-error@^0.8.2",
    "axiod": "https://deno.land/x/axiod@0.26.2/mod.ts",
    "base64/": "https://denopkg.com/chiefbiiko/base64/",
    "bigfloat/": "https://deno.land/x/bigfloat@v3.0.2/",
    "bitcoin": "npm:bitcoinjs-lib@6.1.6",
    "bitcoinjs-lib": "npm:bitcoinjs-lib@6.1.6",
    "buffer": "https://deno.land/std@0.177.0/node/buffer.ts",
    "compress": "https://deno.land/x/foras@v2.1.4/src/deno/mod.ts",
    "constants": "./lib/utils/constants.ts",
    "crypto": "https://deno.land/std@0.177.0/node/crypto.ts",
    "globals": "./globals.d.ts",
    "hooks/": "./islands/hooks/",
    "msgpack": "https://deno.land/x/msgpack@v1.4/mod.ts",
    "preact": "https://esm.sh/preact@10.19.6",
    "preact-render-to-string": "https://esm.sh/*preact-render-to-string@6.1.0",
    "preact/": "https://esm.sh/preact@10.19.6/",
    "redis": "https://deno.land/x/redis@v0.32.4/mod.ts",
    "store/": "./lib/store/",
    "tailwindcss": "npm:tailwindcss@3.4.1",
    "tailwindcss/": "npm:/tailwindcss@3.4.1/",
    "tailwindcss/plugin": "npm:/tailwindcss@3.4.1/plugin.js",
    "tiny-secp256k1": "npm:tiny-secp256k1@2.2.3",
    "utils/": "./lib/utils/"
  },
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "preact",
    "strict": true
  }
}
